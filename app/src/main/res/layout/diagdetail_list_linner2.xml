<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout
        android:id="@+id/fragment_diagdetail_gp2_linners"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/bg_linner"
        android:orientation="horizontal" >
        
        
        <LinearLayout
            android:id="@+id/fragment_diadetail_gp2_profit_linnder"
            style="@style/LinearLayoutStyle1" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="12dp"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/fragment_diadetail_gp2_linnder_profit_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    />

                <TextView
                    android:id="@+id/datas_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:text="新车"
                    android:textSize="11dp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/fragment_diadetail_gp2_linner_profit_right"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical" >

                            <TextView
                                android:id="@+id/fragment_diadetail_gp2_linner_profit_right_name"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="10dp"
                                android:gravity="center_vertical"
                                android:text="GP2"
                                android:textSize="12dp" />

                            <TextView
                                android:id="@+id/tv_gp2_ren"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_alignParentRight="true"
                                android:gravity="center"
                                android:paddingRight="15dp"
                                android:text="%"
                                android:textColor="@color/number_ren_text"
                                android:textSize="9dp" />

                            <TextView
                                android:id="@+id/fragment_diadetail_datas_linner_gp2_right_newspepolnumber"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginRight="2dp"
                                android:layout_toLeftOf="@+id/tv_gp2_ren"
                                android:gravity="center_vertical"
                                android:text="0" 
                                android:textSize="12dp"/>
                        </RelativeLayout>

                        <!-- 团队平均 -->

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical" >

                            <TextView
                                android:id="@+id/fragment_diadetail_gp2_linner_profit_right_names"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="10dp"
                                android:gravity="center_vertical"
                                android:text="团队平均"
                                android:textSize="12dp" />

                            <TextView
                                android:id="@+id/tv_gp2_avg_ren"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_alignParentRight="true"
                                android:gravity="center"
                                android:paddingRight="15dp"
                                android:text="%"
                                android:textColor="@color/number_ren_text"
                                android:textSize="9dp" />

                            <TextView
                                android:id="@+id/fragment_diadetail_gp2_linner_profit_right_averagelnumber"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginRight="2dp"
                                android:layout_toLeftOf="@+id/tv_gp2_avg_ren"
                                android:gravity="center"
                                android:text="0"
                                android:textSize="12dp" />
                        </RelativeLayout>
                
            </LinearLayout>
        </LinearLayout>
        
        <!-- 222222222222222222222222222 -->
        
        <LinearLayout
            android:id="@+id/fragment_diadetail_deal_replacement_linnders"
            style="@style/LinearLayoutStyle1" >


            <LinearLayout
                android:id="@+id/fragment_diadetail_datas_linner_replacement_right"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical" >

                            <TextView
                                android:id="@+id/fragment_diadetail_datas_linner_replacement_right_name"
                                android:layout_width="53dp"
                                android:layout_height="match_parent"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="10dp"
                                android:gravity="center_vertical"
                                android:text="单车毛利"
                                android:textSize="12dp" />

                            <TextView
                                android:id="@+id/tv_maoli_ren"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_alignParentRight="true"
                                android:gravity="center"
                                android:paddingRight="15dp"
                                android:text="元"
                                android:textColor="@color/number_ren_text"
                                android:textSize="9dp" />

                            <TextView
                                android:id="@+id/fragment_diadetail_datas_linner_replacement_right_newspepolnumber"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginRight="2dp"
                                android:layout_toLeftOf="@+id/tv_maoli_ren"
                                android:gravity="center_vertical"
                                android:textColor="@color/text_jiedai"
                                android:text="0" />
                        </RelativeLayout>

                        <!-- 团队平均 -->

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:gravity="center_vertical" >

                            <TextView
                                android:id="@+id/fragment_diadetail_datas_linner_replacement_right_names"
                                android:layout_width="50dp"
                                android:layout_height="match_parent"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentTop="true"
                                android:layout_marginLeft="10dp"
                                android:gravity="center_vertical"
                                android:text="团队平均"
                                android:textSize="12dp" />

                            <TextView
                                android:id="@+id/tv_maoli_avg_ren"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_alignParentRight="true"
                                android:gravity="center"
                                android:paddingRight="15dp"
                                android:text="元"
                                android:textColor="@color/number_ren_text"
                                android:textSize="9dp" />

                            <TextView
                                android:id="@+id/fragment_diadetail_datas_linner_replacement_right_averagelnumber"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginRight="2dp"
                                android:layout_toLeftOf="@+id/tv_maoli_avg_ren"
                                android:gravity="center"
                                android:text="0"
                                android:textSize="12dp" />
                        </RelativeLayout>
                
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
